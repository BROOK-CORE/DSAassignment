#include <stdio.h>

int main() {
    int p, q, i = 0, j = 0;
    scanf("%d", &p);
    int log1[p];
    for(i = 0; i < p; i++) scanf("%d", &log1[i]);
    scanf("%d", &q);
    int log2[q];
    for(j = 0; j < q; j++) scanf("%d", &log2[j]);

    i = j = 0;
    while(i < p && j < q)
        printf("%d ", log1[i] <= log2[j] ? log1[i++] : log2[j++]);
    while(i < p) printf("%d ", log1[i++]);
    while(j < q) printf("%d ", log2[j++]);
}
